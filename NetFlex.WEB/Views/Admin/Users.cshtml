@model IEnumerable<NetFlex.WEB.ViewModels.AdminUserVievModel>

@{
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
	var i = 0;
}

<table class="table table-dark">
	<thead>
		<tr>
			<th scope="col">#</th>
			<th scope="col">First</th>
			<th scope="col">Last</th>
			<th scope="col">Handle</th>
			<th scope="col">Handle</th>
			<th scope="col">Handle</th>
			<th scope="col">Handle</th>
			<th scope="col">Handle</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			i++;
			<tr>
				<th scope="row">@i</th>
				<td>@item.UserId</td>
				<td>@item.UserName</td>
				<td>@item.Email</td>
				<td>@item.Password</td>
				<td>@item.PhoneNumber</td>
				<td>@item.Avatar</td>
				<td>@item.LockoutEnabled</td>
			</tr>
		}
	</tbody>
</table>